<script>

    function quitarCaracteres(cadena) {
        var dato = cadena.replace(/[^a-zA-Z 0-9.]+/g, ' ');
        return dato;
    }

    var tablaSystems;
    $(document).ready(function () {
        tablaSystems = $('#example').DataTable();
        GetSystems("0");
        llenarTablaExcel();
    });
    var datosSystems;
    function GetSystems(All) {
        datosSystems = "";
        var parametros = {
            "All": All
        };
        var sURL = IPWS + "getAllSystems";
        $.ajax({
            url: sURL,
            dataType: "json",
            type: "GET",
            async: false,
            data: parametros,
            headers: {
                Authorization: 'Bearer ' + Token
            },
            success: function (data) {
                if (data[0].ID != null && data[0].ID != 0 ) {
                    var fila;
                    datosSystems = data;
                    tablaSystems.clear();
                    for (var i = 0; i < datosSystems.length; i++) {
                        var RouteEdit = "javascript:UpdateContenedor('Catalogos/EditSystem?ID=" + datosSystems[i].ID + "');";
                        var TextoStatus;
                        var colorBotonStatus;
                        var colorLetraStatus;
                        if (datosSystems[i].Status == true || datosSystems[i].Status == "true") {
                            TextoStatus = "DISABLED";
                            colorBotonStatus = 'btn btn-danger';
                            colorLetraStatus = "<label class='badge badge-success' >ENABLED</label>";
                          
                        } else {
                            TextoStatus = "ENABLED";
                            colorBotonStatus = 'btn btn-success';
                            colorLetraStatus = "<label class='badge bg-danger' >DISABLED</label>";
                        }
                        tablaSystems.row.add([
                            String(((datosSystems[i].ID == "" || datosSystems[i].ID == null) ? "" : datosSystems[i].ID)),
                            String(((quitarCaracteres(datosSystems[i].SistemaName) == "" || quitarCaracteres(datosSystems[i].SistemaName) == null) ? "" : quitarCaracteres(datosSystems[i].SistemaName))),
                            String(((quitarCaracteres(datosSystems[i].Building) == "" || quitarCaracteres(datosSystems[i].Building) == null) ? "" : quitarCaracteres(datosSystems[i].Building))),
                            String(colorLetraStatus),
                            String('<a href=' + RouteEdit + ' type="button" class="btn btn-primary waves-effect waves-light" id="botonModificar" ><span class="btn-label"><i class="fa fa-check"></i></span>Edit</a> <a type="button" class="' + colorBotonStatus + '" onclick="DeleteSystem(' + datosSystems[i].ID + ',' + datosSystems[i].Status + ');"><span class="btn-label"><i class="fa fa-times"></i></span>' + TextoStatus +'</a>'),

                        ]);

                    }
                    tablaSystems.column('1:visible').order('desc').draw();
                   /* $("#example").append(fila);*/
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            }
        });
    }

    function descargarExcel() {
        swal.fire({
            title: "ARE YOU SURE",
            text: "YOU WANT TO DOWLOAD THE FILE?",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    var uri = 'data:application/vnd.ms-excel;base64,',
                        template =
                            '<html xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:x="urn:schemas-microsoft-com:office:excel" xmlns="http://www.w3.org/TR/REC-html40"><head><!--[if gte mso 9]><xml><x:ExcelWorkbook><x:ExcelWorksheets><x:ExcelWorksheet><x:Name>{worksheet}</x:Name><x:WorksheetOptions><x:DisplayGridlines/></x:WorksheetOptions></x:ExcelWorksheet></x:ExcelWorksheets></x:ExcelWorkbook></xml><![endif]--></head><body><table>{table}</table></body></html>',
                        base64 = function (s) {
                            return window.btoa(unescape(encodeURIComponent(s)))
                        },
                        format = function (s, c) {
                            return s.replace(/{(\w+)}/g, function (m, p) {
                                return c[p];
                            })
                        }
                    var toExcel = document.getElementById("tablaC").innerHTML;
                    var ctx = {
                        worksheet: name || '',
                        table: toExcel
                    };
                    var link = document.createElement("a");
                    var fecha = moment(new Date()).subtract('hours').format('MMMM,DD, YYYY');
                    link.download = "SYSTEMS_REPORT_" + fecha + ".xls";
                    link.href = uri + base64(format(template, ctx))
                    link.click();
                } else {
                    swal.fire("WITHOUT CHANGES!");
                }
            });
    }
    function llenarTablaExcel() {
        document.getElementById('fechaReporteTT').innerHTML = "DATE: " + moment(new Date()).subtract('hours').format('MMMM,DD, YYYY');
        document.getElementById('horareporteTT').innerHTML = "HOUR: " + moment(new Date()).subtract('hours').format('HH:mm');
        var id = document.getElementById("ID").checked;
        var system = document.getElementById("SYSTEM").checked;
        var building = document.getElementById("BUILDING").checked;
        var status = document.getElementById("STATUS").checked;
        $("#emp_body1").empty();
        var tr;
        for (var i = 0; i < datosSystems.length; i++) {
            if (datosSystems[i].Status == "true") {
                var status1 = "ENABLED";
            } else {
                status1 = "DISABLED";
            }
            tr = $('<tr/>');
            if (id) {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + datosSystems[i].ID + "</td> ");
            } else {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + "" + "</td> ");
            }
            if (system) {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + quitarCaracteres(datosSystems[i].SistemaName) + "</td> ");
            } else {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + "" + "</td> ");
            }
            if (building) {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + quitarCaracteres(datosSystems[i].Building) + "</td> ");
            } else {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + "" + "</td> ");
            }
            if (status) {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + status1 + "</td> ");
            } else {
                tr.append("<td class='id' style='text-align: center;background: #dddede;border: 1px solid black;font-family:BMWTypeCondensedRegular;'> " + "" + "</td> ");
            }
            $('#emp_body1').append(tr);
        }
    }
    function DeleteSystem(ID, Status) {
        var cambioStatus;
        var ID = ID;
        var TextoStatus;
        if (Status == true) {
            cambioStatus = "false";
            TextoStatus = "DISABLED";
        } else {
            cambioStatus = "true";
            TextoStatus = "ENABLED";
        }
        var para = {
            "ID": ID,
            "Status": cambioStatus,
        };
        Swal.fire({
            title: "ARE YOU SURE",
            text: "YOU WANT TO " + TextoStatus + " THIS SYSTEM?",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: "YES!, " + TextoStatus + ""
        }).then((result) => {
            if (result.isConfirmed) {
                var sURL = IPWS + "DeleteSystem";
                $.ajax({
                    url: sURL,
                    dataType: "json",
                    type: "POST",
                    async: false,
                    data: para,
                    headers: {
                        Authorization: 'Bearer ' + Token
                    },
                    success: function (data) {
                        if (data.Status == "OK") {
                            Swal.fire(
                                'OK!',
                                "THE SYSTEM " + TextoStatus + "  SUCCESSFULLY!",
                                'success'
                            )
                            tablaSystems.clear().draw();
                            GetSystems("0");
                            ocultarelementos = 0;
                        } else {
                            Swal.fire("Error ", "ERROR " + TextoStatus + "  SYSTEM", "error");
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {

                    }
                });
            } else {
                Swal.fire("WITHOUT CHANGES!");
            }
        });
    }

    var ocultarelementos = 0;
    $('#seeAll').click(function () {
       
        if (ocultarelementos == 0) {
            $("#icono").removeClass("glyphicon glyphicon-eye-open");
            $("#icono").addClass("glyphicon glyphicon-eye-close");
            GetSystems("1");
            ocultarelementos = 1;
        } else {
            $("#icono").removeClass("glyphicon glyphicon-eye-close");
            $("#icono").addClass("glyphicon glyphicon-eye-open");
            GetSystems("0");
            ocultarelementos = 0;
        }
       
    });

    function generatePDF() {
        swal.fire({
            title: "ARE YOU SURE",
            text: "YOU WANT TO DOWLOAD THE FILE?",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    datos = [];
                    detail = [];
                    var documentDefinition = {
                        pageSize: 'LETTER',
                        pageOrientation: 'landscape',
                        footer: pieDePagina(),
                        content: [
                            encabezado(),
                            detallesInformacion()
                        ],
                        styles: {
                            tableSubtitulo: {
                                fontSize: 10,
                                color: 'white',
                                alignment: 'center',
                                fillColor: '#233461',
                                lineHeight: [1],
                            },
                            tableText: {
                                fontSize: 9.5,
                                color: '#black',
                                alignment: 'center',
                            },
                        }
                    };
                    pdfMake.createPdf(documentDefinition).download('SYSTEMS_REPORT_"' + moment(new Date()).subtract('hours').format('MMMM,DD, YYYY') + '".pdf');
                } else {
                    swal.fire("WITHOUT CHANGES!");
                }
            });
    }
    var datos = [];
    var detalle = [];
    function getDatosBMW() {
        var detail = [
            [
                {
                    text: 'ID',
                    style: 'tableSubtitulo'
                },
                {
                    text: 'System Name',
                    style: 'tableSubtitulo'
                },
                {
                    text: 'Building',
                    style: 'tableSubtitulo'
                },
                {
                    text: 'Status',
                    style: 'tableSubtitulo'
                }
            ]
        ];
        for (let index = 0; index < datosSystems.length; index++) {

            var id = document.getElementById("ID").checked;
            var system = document.getElementById("SYSTEM").checked;
            var building = document.getElementById("BUILDING").checked;
            var status = document.getElementById("STATUS").checked;

            if (datosSystems[index].Status == "true") {
                var status1 = "ENABLED";
            } else {
                status1 = "DISABLED";
            }

            if (id) {
                datos.push({
                    text: datosSystems[index].ID,
                    style: 'tableText'
                });
            } else {
                datos.push({
                    text: '',
                    style: 'tableText'
                });
            }
            if (system) {
                datos.push({
                    text: quitarCaracteres(datosSystems[index].SistemaName),
                    style: 'tableText'
                });
            } else {
                datos.push({
                    text: '',
                    style: 'tableText'
                });
            }
            if (building) {
                datos.push({
                    text: quitarCaracteres(datosSystems[index].Building),
                    style: 'tableText'
                });
            } else {
                datos.push({
                    text: '',
                    style: 'tableText'
                });
            }
            if (status) {
                datos.push({
                    text: status1,
                    style: 'tableText'
                });
            } else {
                datos.push({
                    text: '',
                    style: 'tableText'
                });
            }


            detail.push(datos);
            datos = [];
        }
        return detail;
    }
    function detallesInformacion() {
        return infDetallesTabla = [{
            margin: [0, 20, 0, 0],
            table: {
                widths: ['*', '*', '*', '*'],
                body: getDatosBMW()
            },
            layout: {
                hLineWidth: function (i, node) {
                    return (i === 0 || i === node.table.body.length) ? 1 : 1;
                },
                vLineWidth: function (i, node) {
                    return (i === 0 || i === node.table.widths.length) ? 1 : 1;
                },
                hLineColor: function (i, node) {
                    return (i === 0 || i === node.table.body.length) ? '#ccc' : '#ccc';
                },
                vLineColor: function (i, node) {
                    return (i === 0 || i === node.table.widths.length) ? '#ccc' : '#ccc';
                },
                fillColor: function (rowIndex, node, columnIndex) {
                    return (rowIndex % 2 === 0) ? '#dddede' : null;
                }
            }
        }];
    }

